{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "app/templates/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/logo-assets/(.*)",
      "dest": "app/templates/logo/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "api/index.py"
    },
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ],
  "env": {
    "PYTHONPATH": "/var/task"
  }
}
